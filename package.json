{
  "name": "mashupchallenge-demo",
  "version": "0.0.1",
  "description": "Mashup Challenge Demo",
  "scripts": {
    "start": "sudo node server.js & sudo node smartcomposition/Node.SynchronizationServer.js",
    "postinstall": "node -e \"var srcpath='node_modules/smartcomposition';var dstpath='smartcomposition';var fs=require('fs');fs.readlink(dstpath,function(err, existLink){if(!existLink){fs.symlinkSync(srcpath, dstpath,'dir');}});\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michikrug/mashupchallenge-demo.git"
  },
  "author": "Michael Krug",
  "bugs": {
    "url": "https://github.com/michikrug/mashupchallenge-demo/issues"
  },
  "homepage": "https://github.com/michikrug/mashupchallenge-demo#readme",
  "dependencies": {
    "connect": "^3.4.1",
    "serve-static": "^1.10.2",
    "smartcomposition": "git://github.com/michikrug/SmartComposition.git#develop"
  }
}
